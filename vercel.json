{
  "version": 2,
  "buildCommand": "cd web && npm install && npm run build && cd .. && node build-for-vercel.js",
  "outputDirectory": "vercel-build",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/dist/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}